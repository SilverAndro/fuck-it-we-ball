{
	"schema_version": 1,
	"quilt_loader": {
		"group": "io.github.silverandro",
		"id": "shiv",
		"version": "${version}",
		"metadata": {
			"name": "Shiv",
			"description": "Fixes various quilt incompatibilities in other mods. Its like a shim but more dangerous",
			"contributors": {
				"Silver": "Owner"
			},
			"contact": {
				"sources": "https://github.com/SilverAndro/shiv",
				"issues": "https://github.com/SilverAndro/shiv/issues"
			},
			"icon": "assets/shiv/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"init": "io.github.silverandro.shiv.ShivInit",
			"pre_launch": "io.github.silverandro.shiv.ShivInit"
		},
		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=0.18.1-"
			}
		]
	},
	"mixin": "shiv.mixins.json"
}
